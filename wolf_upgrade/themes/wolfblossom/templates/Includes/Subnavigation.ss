<ul>	<% control Menu(2) %>		<li class="$LinkingMode"><a href="$Link" title="$Title">$MenuTitle</a>		<% if Children %>		<ul>			<% control Children %>				<li class="$LinkingMode"><a href="$Link" title="$Title">$MenuTitle</a>				<% if Children %>					<ul>						<% control Children %>							<li class="$LinkingMode"><a href="$Link" title="$Title">$MenuTitle</a>								<% if Children %>									<ul>										<% control Children %>											<li class="$LinkingMode"><a href="$Link" title="$Title">$MenuTitle</a></li>										<% end_control %>									</ul>								<% end_if %>							</li>						<% end_control %>					</ul>				<% end_if %>				</li>			<% end_control %>		</ul>		<% end_if %>	</li>	<% end_control %></ul>